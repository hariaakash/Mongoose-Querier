{
    "source": {
        "include": ["./lib"],
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [ "plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/jsdocs/",
        "template": "node_modules/better-docs"
    }
}